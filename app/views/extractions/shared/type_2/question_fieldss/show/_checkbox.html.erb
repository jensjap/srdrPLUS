<!-- This needs to be converted to a form. -->
<!-- Iterate through all QuestionRowColumnFieldsQuestionRowColumnFieldOptions with QuestionRowColumnFieldOption 'answer_choice' -->
<% qrc.question_row_columns_question_row_column_options.where(question_row_column_option_id: 1).each do |qrcfqrcfo| %>
  <%= check_box_tag("checkbox-#{qrc.id}", nil, false, data: { prereq: "prereq-#{qrcfqrcfo.class}-#{qrcfqrcfo.id}" }) %>
  <%= qrcfqrcfo.name %><br>
<% end %>
