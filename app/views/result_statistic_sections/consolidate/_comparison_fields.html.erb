<div class="nested-fields comparate-groups">
  <%= f.simple_fields_for :comparate_groups do |comparate_group| %>
    <%= render "result_statistic_sections/edit/comparate_group_fields", f: comparate_group %>
  <% end %>
  <div class="links add-comparate-group">
    <%= link_to_add_association 'add comparate group', f, :comparate_groups,
      partial: "result_statistic_sections/edit/comparate_group_fields",
      render_options: { locals: { comparison: comparison } } %>
  </div>
  <%= f.button :submit, t('save_comparison'), class: 'button small' %>
</div>
