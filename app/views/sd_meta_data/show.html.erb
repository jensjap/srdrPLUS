<div class="column row" style="margin-bottom: 3vh;">
  <%#= image_tag('SRDR 2.0 Placeholder.png', size: '100x100', class: 'right', style: "float: right;") %>
  <h6 style="font-weight: bold; margin: 20px 0 0 0;">SRDR+ Project</h6>
  <%= link_to(@project.try(:name) || 'None', edit_project_path(@project), target: '_blank') %>
  <h6 style="font-weight: bold; margin: 20px 0 0;">Source Systematic Review Report</h6>
  <% if @systematic_review_report %>
    <a data-toggle='offCanvasRightReportHtml'>
      <%= @sd_meta_datum.try(:report_title) || @report.try(:title) || @sd_meta_datum.report_file.filename ||  'None' %>
    </a>
  <% end %>
</div>
<div class="column row" id="complete-form">
  <%= render 'sd_meta_data/show/form' %>
</div>
