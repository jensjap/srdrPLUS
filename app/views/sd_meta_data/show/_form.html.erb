<div class="grid-x align-center" style="font-size: small !important;">
  <div class="cell medium-2">
    <ul id="vertical-tabs" class="tabs vertical">
      <li class="tabs-title">
        <%= link_to 'Title, Funding Sources, and Dates', '?panel_number=0', class: (@panel_number == 0 ? 'bg-light-grey' : ''), id: "panel-0-label" %>
      </li>
      <li class="tabs-title">
        <%= link_to 'Authors and Stakeholders', '?panel_number=1', class: (@panel_number == 1 ? 'bg-light-grey' : ''), id: "panel-1-label" %>
      </li>
      <li class="tabs-title">
        <%= link_to 'URL Links', '?panel_number=2', class: (@panel_number == 2 ? 'bg-light-grey' : ''), id: "panel-2-label" %>
      </li>
      <li class="tabs-title">
        <%= link_to "Purpose, Analytic Framework, and Key Questions", '?panel_number=3', class: (@panel_number == 3 ? 'bg-light-grey' : ''), id: "panel-3-label" %>
      </li>
      <li class="tabs-title">
        <%= link_to "PICODTS", '?panel_number=4', class: (@panel_number == 4 ? 'bg-light-grey' : ''), id: "panel-4-label" %>
      </li>
      <li class="tabs-title">
        <%= link_to 'Search Strategy and Results of Screening', '?panel_number=6', class: (@panel_number == 6 ? 'bg-light-grey' : ''), id: "panel-6-label" %>
      </li>
      <li class="tabs-title">
        <%= link_to 'Risk of Bias and Overall Summary of Evidence', '?panel_number=7', class: (@panel_number == 7 ? 'bg-light-grey' : ''), id: "panel-7-label" %>
      </li>
      <li class="tabs-title">
        <%= link_to 'Results for Individual Outcomes', '?panel_number=8', class: (@panel_number == 8 ? 'bg-light-grey' : ''), id: "panel-8-label" %>
      </li>
    </ul>
  </div>
  <div class="cell medium-10">
    <div class="tabs-content vertical">
      <div class="grid-form">
        <%= render partial: "sd_meta_data/show/panel_#{@panel_number}", locals: { sd_meta_datum: @sd_meta_datum, project: @project } %>
      </div>
    </div>
  </div>
</div>
<script>
  $(document).on("click", ".openable", function() {
    window.open($(this).attr('src'));
  });
</script>
