<div id="panel-2" class="tabs-panel is-active">
  <fieldset>
    <legend>URL Links</legend>
    <div data-row-span="2">
      <div data-field-span="1">
        <label>PROSPERO</label>
        <div><%= sd_url_for @sd_meta_datum.prospero_link %></div>
      </div>
      <div data-field-span="1">
        <label>Protocol</label>
        <div><%= sd_url_for @sd_meta_datum.protocol_link %></div>
      </div>
    </div>
    <div data-row-span="2">
      <div data-field-span="1">
        <label>Full Report</label>
        <div><%= sd_url_for @sd_meta_datum.full_report_link %></div>
      </div>
      <div data-field-span="1">
        <label>Structured Abstract</label>
        <div><%= sd_url_for @sd_meta_datum.structured_abstract_link %></div>
      </div>
    </div>
    <div data-row-span="2">
      <div data-field-span="1">
        <label>Main Points or Key Messages</label>
        <div><%= sd_url_for @sd_meta_datum.key_messages_link %></div>
      </div>
      <div data-field-span="1">
        <label>Lay Abstract / Lay Language Summary</label>
        <div><%= sd_url_for @sd_meta_datum.abstract_summary_link %></div>
      </div>
    </div>
    <div class="sd-show-inner">
      <div data-row-span="1">
        <div data-field-span="1">
          <label>Evidence Summary / Executive Summary</label>
          <div><%= sd_url_for @sd_meta_datum.evidence_summary_link %></div>
        </div>
      </div>
    </div>
    <div data-row-span="2">
      <div data-field-span="1">
        <label>Journal Articles</label>
        <% if @sd_meta_datum.sd_journal_article_urls.empty? %>
          <div>-----</div>
        <% else %>
          <td>
            <% @sd_meta_datum.sd_journal_article_urls.each do |sd_journal_article_url| %>
              <li><%= sd_url_for sd_journal_article_url.name %></li>
            <% end %>
          </td>
        <% end %>
      </div>
      <div data-field-span="1">
        <label>Other Items</label>
        <% if @sd_meta_datum.sd_other_items.empty? %>
          <div>-----</div>
        <% else %>
          <td>
            <% @sd_meta_datum.sd_other_items.each do |sd_other_item| %>
              <li><%= sd_url_for sd_other_item.url, sd_other_item.name %></li>
            <% end %>
          </td>
        <% end %>
      </div>
    </div>
    <div data-row-span="2">
      <div data-field-span="1">
        <label>Disposition of Comments</label>
        <div><%= sd_url_for @sd_meta_datum.disposition_of_comments_link %></div>
      </div>
      <div data-field-span="1">
        <label>Downloadable Content</label>
        <div><%= sd_url_for @sd_meta_datum.srdr_data_link %></div>
      </div>
    </div>
    <div data-row-span="2">
      <div data-field-span="1">
        <label>Most Recent Previous Version of SRDR+ Data</label>
        <div><%= sd_url_for @sd_meta_datum.most_previous_version_srdr_link %></div>
      </div>
      <div data-field-span="1">
        <label>Most Recent Previous Version of Full Report</label>
        <div><%= sd_url_for @sd_meta_datum.most_previous_version_full_report_link %></div>
      </div>
    </div>
  </fieldset>
</div>
