<div id="panel-7" class="tabs-panel is-active">
  <fieldset>
    <div style="display:flex; flex-direction: row;">
      <legend>Risk of Bias and Overall Summary of Evidence</legend>
      <div class="status-switch" id="7-yes-no-section" class="<%= @sd_meta_datum.section_flag_7 ? 'completed' : 'draft' %>" data-sd_meta_datum_id="<%= @sd_meta_datum.id %>">
        <span class="status-label">
          <% if @sd_meta_datum.section_flag_7 %>
            Completed
          <% else %>
            Draft
          <% end %>
        </span>
      </div>
    </div>
    <div data-row-span="1">
      <div data-field-span="1">
        <label>Summaries of Evidence</label>
        <div class="cocoon-container result-list">
          <%= render 'sd_meta_data/form/nested_associations/result_list', sd_meta_datum: @sd_meta_datum, url: @url %>
        </div>
      </div>
    </div>
  </fieldset>
</div>
