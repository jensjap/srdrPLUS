<% data_audit = project.data_audit %>
<% published_at = project&.publishing&.approval&.created_at %>
<%= form_with(model: data_audit,
              url: data_audit.persisted? ? data_audit_path(data_audit) : project_data_audit_path(project),
              local: false) do |f| %>
  <td><%= project.id %></td>
  <td><%= project.name %></td>
  <td><%= published_at %></td>
  <% if published_at.present? && project.created_at.present? %>
    <td><%= (published_at - project.created_at).to_i %></td>
  <% else %>
    <td>N/A</td>
  <% end %>
  <td><%= f.check_box :epc_source %></td>
  <td><%= f.text_field :epc_name %></td>
  <td><%= f.text_field :non_epc_name %></td>
  <td><%= project.key_questions.count %></td>
  <td><%= project.citations.count %></td>
  <td><%= project.extractions.reject { |extraction| extraction.consolidated }.count %></td>
  <td><%= project.extractions.reject { |extraction| !extraction.consolidated }.count %></td>
  <td><%= f.text_field :capture_method %></td>
  <td><%= project.pct_extractions_with_no_data %></td>
  <td><%= f.text_field :pct_extractions_with_unstructured_data %></td>
  <td><%= f.select :distiller_w_results,
                  DataAudit::DISTILLER_W_RESULTS_OPTIONS,
                  { selected: data_audit.distiller_w_results, prompt: "-- Select --" } %></td>
  <td><%= f.select :single_multiple_w_consolidation,
                  DataAudit::SINGLE_MULTIPLE_W_CONSOLIDATION,
                  { selected: data_audit.single_multiple_w_consolidation, prompt: "-- Select --" } %></td>
  <td><%= project.sd_meta_data.present? %></td>
  <td><%= f.text_area :notes %></td>
  <td><%= f.submit nil, class: 'button' %></td>
<% end %>
