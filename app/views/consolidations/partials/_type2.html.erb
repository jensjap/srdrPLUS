<div class="flex flex-col">
  <div class="-my-2 -mx-4 overflow-x-auto sm:-mx-6 lg:-mx-8">
    <div class="min-w-full md:px-6 lg:px-8 space-y-8">
      <!-- prettier-ignore -->
      <% efps.questions.each_with_index do |question, index| %>
      <div class="">
        <div>
          <h2 class="text-base"><%= "#{index + 1}. #{question.name}" %></h2>
          <h3 class="text-sm text-gray-500"><%= question.description %></h3>
        </div>
        <% question.question_rows.each do |qr| %>
        <div class="flex">
          <% qr.question_row_columns.each do |qrc| %>
          <div>
            <!-- prettier-ignore -->
            <%= render('consolidations/partials/checkbox', qrc:) if qrc.question_row_column_type.name =='checkbox' %>
            <%= render('consolidations/partials/dropdown', qrc:) if qrc.question_row_column_type.name =='dropdown' %>
            <%= render('consolidations/partials/numeric', qrc:) if qrc.question_row_column_type.name =='numeric' %>
            <%= render('consolidations/partials/radio', qrc:) if qrc.question_row_column_type.name =='radio' %>
            <%= render('consolidations/partials/select2_multi', qrc:) if qrc.question_row_column_type.name =='select2_multi' %>
            <%= render('consolidations/partials/select2_single', qrc:) if qrc.question_row_column_type.name =='select2_single' %>
            <%= render('consolidations/partials/text', qrc:) if qrc.question_row_column_type.name =='text' %>
          </div>
          <% end %>
        </div>
        <% end %>
      </div>
      <% end %>
    </div>
  </div>
</div>
