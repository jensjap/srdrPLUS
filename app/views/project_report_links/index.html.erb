<div class="column row" id="sdMetaData-list">
  <h3>Completed Project-Report Links</h3>
  <table class="hover">
    <thead>
      <tr>
        <th style="width: 45%;">SRDR+ Project</th>
        <th style="width: 45%;">Systematic Review Report</th>
        <th style="width: 5%;"></th>
        <th style="width: 5%;"></th>
      </tr>
    </thead>
    <tbody style="font-size: 12px;">
      <% if @sd_meta_data.empty? %>
        <tr>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
        </tr>
      <% else %>
        <% @sd_meta_data.each do |sd_meta_datum| %>
          <tr>
            <td><%= sd_meta_datum.try(:project).try(:name) %></td>
            <td><%= sd_meta_datum.try(:report).try(:title) %></td>
            <% if sd_meta_datum.project %>
              <td><%= link_to('View', sd_meta_datum, class: 'button') %></td>
              <td><%= link_to('Query', project_report_link_new_query_form_path(sd_meta_datum), class: 'button') %></td>
            <% else %>
              <td></td>
              <td><div>Project Required</div></td>
            <% end %>
          </tr>
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
